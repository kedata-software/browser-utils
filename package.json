{
  "name": "@kedata-software/browser-utils",
  "description": "A set of utilities for browser",
  "version": "0.0.4",
  "private": false,
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "build:pre": "rm -rf dist",
    "build:tsdown": "tsdown",
    "build": "pnpm build:pre && pnpm build:tsdown",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/node": "^22.14.1",
    "jsdom": "^26.0.0",
    "release-it": "^19.0.4",
    "typescript": "^5.7.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "blob-polyfill": "^9.0.20240710",
    "tsdown": "^0.13.2"
  }
}
